#include <iostream>
#include <queue>
#include <stdio.h>
using namespace std;

int main()
{
    cin.tie(NULL);
    int n;
    scanf("%d", &n);

    priority_queue<int> x;
    int a;
    for (int i = 0; i < n; i++)
    {
        scanf("%d", &a);

        if (a == 0 && x.empty()==1)
        {
            printf("%d\n", 0);
        }
        else if (a==0)
        {
            printf("%d\n", x.top());
            x.pop();
        }
        else
            x.push(a);
    }
    return 0;
}
